        <li><a data-toggle="tab" href="#recrutamento"><i class="fa fa-spinner fa-2x" aria-hidden="true" title="Recrutamento"></i></a></li>
        <li><a data-toggle="tab" href="#notas"><i class="fa fa-sticky-note-o fa-2x" aria-hidden="true" title="Notas"></i></a></li>

    </ul>

</div>

<!-- Tab content -->
<div class="tab-content" id="myTab">

    <div id="resumo" class="tab-pane fade in active">
        <div style="text-align: left;
                        background-color: #428bca;
                        padding: 10px;
                        border-bottom-left-radius: 5px;
                        border-bottom-right-radius: 5px;
                        color: white;
                        margin-top: 0;
                        min-height:55px;">
            <h3 style="margin-top:0; margin-bottom:0;padding:3px; float:left;">
                Resumo de @Model.NomeCliente
            </h3>

        </div>

        <dl class="dl-horizontal">
            <dt>
                Cliente Rainbow
            </dt>
            <dd>
                @if (Model.ClienteRainbow)
                {
                    <p>Sim</p>
                }
                else
                {
                    <p>Não</p>
                }
            </dd>

            <dt>
                Recomendado por:
            </dt>

            <dd>
                <p>@Model.RecomendadoPor</p>
            </dd>

            <dt>
                Recomendações
            </dt>

            <dd>
                @if (Model.Recomendacao.Count > 0)
                {
                    @Html.DisplayFor(model => model.Recomendacao.Count)
                }
                else
                {
                    <p style="color:red;">Sem recomendações</p>
                }
            </dd>

            <dt>
                <span style="color:green;">OK's</span>
            </dt>

            <dd>
                @{
                    int contagem = 0;
                    foreach (var cliente in Model.Recomendacao)
                    {
                        if (cliente.OkParaContactar)
                        {
                            contagem++;
                        }
                    }
                    <p>@contagem</p>
                }
            </dd>

            <dt>
                <span style="color: red;">Não OK's</span>
            </dt>

            <dd>
                @{
                    int NOK = 0;

                    foreach (var cliente in Model.Recomendacao)
                    {
                        if (!cliente.OkParaContactar)
                        {
                            NOK++;
                        }
                    }
                    <p>@NOK</p>
                }
            </dd>

            <dt>
                Data última Demo
            </dt>
            <dd>
                @{
                    string ultimaDemo = null;
                    try
                    {
                        ultimaDemo = Model.Demonstracao.LastOrDefault().DataMarcacao.ToShortDateString();
                    }
                    catch (Exception)
                    {
                        <p>Sem demos efectuadas</p>
                    }

                    <p>@ultimaDemo</p>
                }
            </dd>

            <dt>
                Filhos dependentes:
            </dt>
            <dd>
                @{
                    int filhos = 0;

                    foreach (var membro in Model.MembroFamilia)
                    {
                        if (membro.TipoMembroFamiliaId == 1)
                        {
                            filhos++;
                        }
                    }
                    <p>@filhos</p>
                }
            </dd>

            <dt>
                Animais estimaçao
            </dt>
            <dd>
                @{
        int animais = 0;

        foreach (var animal in Model.AnimalEstimacao)
        {
            animais++;
        }
            @*<p>@animais</p>*@

if (animais > 0)
{
            <img style="width:30px;" title="Cliente tem animais de estimação" src="~/Content/Images/on.png" />
}
else
{
            <img style="width:30px;" title="Cliente não tem animais de estimação" src="~/Content/Images/off.png" />
}
                }
            </dd>
        </dl>

    </div>

