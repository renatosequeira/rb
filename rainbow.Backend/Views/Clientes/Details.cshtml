        <li class="active"><a data-toggle="tab" href="#resumo"><i class="fa fa-list fa-2x" aria-hidden="true" title="Resumo"></i></a></li>
        <li><a data-toggle="tab" href="#contactos"><i class="fa fa-address-book fa-2x" aria-hidden="true" title="Contactos"></i></a></li>
        <li><a data-toggle="tab" href="#dadosFinanceiros"><i class="fa fa-eur fa-2x" aria-hidden="true" title="Dados financeiros"></i></a></li>
        <li><a data-toggle="tab" href="#dadosFamiliares"><i class="fa fa-users fa-2x" aria-hidden="true" title="Dados Familiares"></i></a></li>
        <li><a data-toggle="tab" href="#animaisEstimacao"><i class="fa fa-paw fa-2x" aria-hidden="true" title="Animais de estimação"></i></a></li>
        <li><a data-toggle="tab" href="#demos"><i class="fa fa-eye fa-2x" aria-hidden="true" title="Demos"></i></a></li>
        <li><a data-toggle="tab" href="#recomendacoes"><i class="fa fa-user-plus fa-2x" aria-hidden="true" title="Recomendações"></i></a></li>
