@model rainbow.Domain.Client.Cliente

@{
    ViewBag.Title = "Details";
}

<link href="~/Content/font-awesome-4.0.3/css/font-awesome.css" rel="stylesheet" />

<style>
    body {
        font-family: Arial;
    }

    /* Style the tab */
    .tab {
        overflow: hidden;
        border: 1px solid #ccc;
        background-color: #f1f1f1;
    }

        /* Style the buttons inside the tab */
        .tab button {
            background-color: inherit;
            float: left;
            border: none;
            outline: none;
            cursor: pointer;
            padding: 14px 16px;
            transition: 0.3s;
            font-size: 17px;
        }

            /* Change background color of buttons on hover */
            .tab button:hover {
                background-color: #ddd;
            }

            /* Create an active/current tablink class */
            .tab button.active {
                background-color: #ccc;
            }

    /* Style the tab content */
    .tabcontent {
        display: none;
        padding: 6px 12px;
        border: 1px solid #ccc;
        border-top: none;
    }

    .OkImage img {
        width: 25px;
    }

    .SpecialNote img {
        width: 30px;
        float: right;
    }

    .recrutamento img {
        width: 25px;
    }
</style>

<div>
    <div>
        <h2>
            @Html.DisplayFor(model => model.Title.TitleName)
            @Html.DisplayFor(model => model.NomeCliente)

            @if ((Convert.ToInt32(Model.IdadeCliente) >= 30) && (Convert.ToInt32(Model.IdadeCliente) <= 76))
            {
                if (Model.EstadoCivilId != 5 && !string.IsNullOrEmpty(Model.TelemovelCliente))
                {
                    <img style="float:right; width:100px;" src="~/Content/Images/Qualified.png" />
                }

            }

        </h2>

    </div>
    <hr />
    <dl class="dl-horizontal">

        <dt>
            @Html.DisplayNameFor(model => model.EstadoCivil.NomeEstadoCivil)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.EstadoCivil.NomeEstadoCivil)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Profissao.NomeProfissao)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Profissao.NomeProfissao)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.DataNascimentoCliente)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DataNascimentoCliente)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.IdadeCliente)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.IdadeCliente)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.ElegivelParaRecrutamento)
        </dt>

        <dd class="recrutamento">
            @if (Model.ElegivelParaRecrutamento)
            {
                <img title="Eligível para recrutar" src="~/Content/Images/green-light.png" />
            }
            else
            {
                <img title="Não Eligível para recrutar" src="~/Content/Images/red-light.png" />
            }
        </dd>

        <dd>
            OK para contacto
        </dd>

        <dt>
            @{
                bool ok;
                string OkString;
                foreach (var item in Model.Recomendacao)
                {
                    if (item.OkParaContactar)
                    {
                        ok = true;
                        OkString = "Ok";
                    }
                }

            }
        </dt>

    </dl>


</div>


<p style="float: right;">
    @*@Html.ActionLink("Edit", "Edit", new { id = Model.ClientId }) |
        @Html.ActionLink("Back to List", "Index") |*@
    @Html.ActionLink(" ", "Edit", "Clientes", new { id = Model.ClientId }, new { @class = "btn btn-success btn-lg glyphicon glyphicon-edit", title = "Editar " + Model.NomeCliente }) |
    @Html.ActionLink(" ", "Index", "Clientes", new { id = Model.ClientId }, new { @class = "btn btn-danger btn-lg glyphicon glyphicon-chevron-left", title = "Voltar" })
</p>


<!-- Tab links -->
<div class="tab" style="clear:both;">
